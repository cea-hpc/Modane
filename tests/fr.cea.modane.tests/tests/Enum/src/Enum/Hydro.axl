<?xml version="1.0" encoding="utf-8"?><module name="Hydro" version="1.0" namespace-name="Enum" autoload="false">
  <description>Mon module Hydro</description>
  <userclass>User</userclass>
  <name lang="fr">hydrodynamique</name>
  <variables>
    <variable field-name="old_cell_volume" name="OldCellVolume" data-type="real" item-kind="none" dim="0" dump="true" need-sync="true" restore="false" execution-depend="false">
      <description>Vieux volume</description>
    </variable>
  </variables>
  <entry-points>
    <entry-point method-name="compute" name="Compute" where="compute-loop" property="none"/>
  </entry-points>
  <options>
    <enumeration name="c-qs-computing-method" type="::Enum::CQsComputingMethod" default="t4">
      <userclass>User</userclass>
      <description>Méthode de calcul des résultantes aux sommets des mailles.</description>
      <enumvalue genvalue="::Enum::CQsComputingMethod::Iso" name="iso"/>
      <enumvalue genvalue="::Enum::CQsComputingMethod::T4" name="t4"/>
      <enumvalue genvalue="::Enum::CQsComputingMethod::Pracht" name="pracht"/>
      <enumvalue genvalue="::Enum::CQsComputingMethod::PrachtPmt" name="pracht-pmt"/>
      <enumvalue genvalue="::Enum::CQsComputingMethod::PrachtCompatible" name="pracht-compatible">
        <userclass>User</userclass>
      </enumvalue>
      <enumvalue genvalue="::Enum::CQsComputingMethod::Symetrie" name="symetrie"/>
      <enumvalue genvalue="::Enum::CQsComputingMethod::Enum2Computing" name="enum2-computing"/>
      <enumvalue genvalue="::Enum::CQsComputingMethod::Toto2T2" name="toto2-t2"/>
    </enumeration>
    <simple name="fine-node-mass-computing" type="bool" default="true">
      <description>Calcule les masses nodales en utilisant les sous volumes de contrôle. Dans le cas où cette option est désactivée, les masses aux noeuds sont	évaluées par 1/8ème de la masse des mailles qui entourent chaque noeud pour les mailles hexaédriques, et de façon similaire pour les autres types de mailles.</description>
    </simple>
    <complex name="pseudo-viscosity" interface="::Enum::IPseudoViscosity" type="PseudoViscosity">
      <enumeration name="type" type="::EnumSubEnum::PseudoViscosityType" default="none">
        <enumvalue genvalue="::EnumSubEnum::PseudoViscosityType::None" name="none"/>
        <enumvalue genvalue="::EnumSubEnum::PseudoViscosityType::ScalarCellDiv" name="scalar-cell-div"/>
        <enumvalue genvalue="::EnumSubEnum::PseudoViscosityType::ScalarCellWilkins" name="scalar-cell-wilkins"/>
        <enumvalue genvalue="::EnumSubEnum::PseudoViscosityType::TensorEdge" name="tensor-edge"/>
        <enumvalue genvalue="::EnumSubEnum::PseudoViscosityType::TensorTvdEdge" name="tensor-tvd-edge"/>
      </enumeration>
      <enumeration name="limiter-tvd" type="::Enum::PseudoViscosityLimiter" default="christensen">
        <enumvalue genvalue="::Enum::PseudoViscosityLimiter::None" name="none"/>
        <enumvalue genvalue="::Enum::PseudoViscosityLimiter::Christensen" name="christensen"/>
        <enumvalue genvalue="::Enum::PseudoViscosityLimiter::ChakravartyOsher" name="chakravarty-osher"/>
        <enumvalue genvalue="::Enum::PseudoViscosityLimiter::Superbee" name="superbee"/>
        <enumvalue genvalue="::Enum::PseudoViscosityLimiter::Minmod" name="minmod"/>
        <enumvalue genvalue="::Enum::PseudoViscosityLimiter::Vanleer" name="vanleer">
          <description>Limiteur de VanLeer</description>
        </enumvalue>
      </enumeration>
    </complex>
  </options>
</module>
